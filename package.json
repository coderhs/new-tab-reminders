{
  "name": "new-tab-reminder",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build && yarn copy-icon_128",
    "build-google": "yarn build && rm -rf ./dist-google && cp -rf ./dist ./dist-google && yarn manifest-v3",
    "build-firefox": "yarn build && rm -rf ./dist-firefox && cp -rf ./dist ./dist-firefox && yarn manifest-v2",
    "lint": "vue-cli-service lint",
    "copy-icon_128": "cp ./src/assets/icon_128.png ./dist/",
    "manifest-v3": "cp ./src/manifest_v3.json ./dist-google/manifest.json",
    "manifest-v2": "cp ./src/manifest_v2.json ./dist-firefox/manifest.json"
  },
  "dependencies": {
    "vue": "^2.5.22"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "3.6.0",
    "@vue/cli-plugin-eslint": "3.6.0",
    "@vue/cli-service": "3.6.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "keywords": [],
  "description": ""
}
