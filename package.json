{
  "name": "new-tab-reminder",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build && yarn copy-icon_128",
    "build-google": "yarn build && rm -rf ./dist-google && cp -rf ./dist ./dist-google && yarn manifest-v3",
    "build-firefox": "yarn build && rm -rf ./dist-firefox && cp -rf ./dist ./dist-firefox && yarn manifest-v2",
    "lint": "vue-cli-service lint",
    "copy-icon_128": "cp ./src/assets/icon_128.png ./dist/",
    "manifest-v3": "cp ./src/manifest_v3.json ./dist-google/manifest.json",
    "manifest-v2": "cp ./src/manifest_v2.json ./dist-firefox/manifest.json"
  },
  "dependencies": {
    "core-js": "^3.8.3",
    "loadash": "^1.0.0",
    "vue": "^2.6.14"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "vue-template-compiler": "^2.6.14"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "engines": {
    "node": ">=16.14.2",
    "npm": ">=8.5.5"
  }
}
